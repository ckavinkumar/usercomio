<html>

<head>
	 <% include commoncss.ejs %>
	 
    <link rel="stylesheet" type="text/css" href="/css/internal/userregistration.css" />

    <!-- external scripts -->
    <% include commonscripts.ejs %>

    <!--- internal scripts --->
    <script type="text/javascript" src="/js/src/internal/userregistration.js"></script>
  </head>
  <body>
  	<div class="UC_UserRegistrationContainerCls">
    	
    	<div class="form-group" style="overflow:auto">
			<label for="inputEmail3" class="col-sm-2 control-label">Full Name</label>
			<div class="col-sm-10">
				<input type="email" class="form-control" id="ucuserreg_fullnameinput" placeholder="Full Name">
			</div>
		</div>
    	<div class="form-group" style="overflow:auto">
			<label for="inputEmail3" class="col-sm-2 control-label">Email</label>
			<div class="col-sm-10">
				<input type="email" class="form-control" id="ucuserreg_emailinput" placeholder="Email">
			</div>
		</div>
		<div class="form-group" style="overflow:auto">
			<label for="inputPassword3" class="col-sm-2 control-label">Password</label>
			<div class="col-sm-10">
				<input type="password" class="form-control" id="ucuserreg_passwordinput" placeholder="Password">
			</div>
		</div>
		<div class="form-group">
			<label for="inputPassword3" class="col-sm-2 control-label">Confirm Password</label>
			<div class="col-sm-10">
				<input type="password" class="form-control" id="ucuserreg_confirmpasswordinput" placeholder="Password">
			</div>
		</div>
		
		<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<button type="submit" id="ucuserreg_submitbtn" class="btn btn-default">Register</button>
			</div>
		</div>
    </div>	
    
    
    <script>
	    var uc_userregistration  = new UC_UserRegistrationController();
	    uc_userregistration.constructor();
  </script>
  </body>
</html>
